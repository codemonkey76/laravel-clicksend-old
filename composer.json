{
    "name": "codemonkey76/laravel-clicksend",
    "description": "Laravel wrapper for ClickSend",
    "keywords": [
        "codemonkey76",
        "laravel-clicksend"
    ],
    "homepage": "https://github.com/codemonkey76/laravel-clicksend",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Shane Poppleton",
            "email": "shane@alphasg.com.au",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "clicksend/clicksend-php": "^5.0",
        "illuminate/support": "^6|^7|^8",
        "illuminate/http": "^5|^6|^7|^8"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Codemonkey76\\Clicksend\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Codemonkey76\\Clicksend\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
              "Codemonkey76\\ClickSend\\ClickSendServiceProvider"
            ]
        }
    }
}
